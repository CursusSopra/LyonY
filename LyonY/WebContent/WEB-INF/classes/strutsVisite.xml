<?xml version="1.0" encoding="UTF-8"?>

<struts>

	<constant name="struts.devMode" value="true" />

<package name="lyonYVisites" extends="struts-default">
		<result-types>
			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
		</result-types>



		<!-- Affichage de la liste des monuments -->
		<action name="listeDesMonuments" class="fr.cursusSopra.action.ListeDesMonumentsAction"
			<result name="success" type="tiles">liste des monuments</result>
		</action>
		
	</package>